    <Button
        android:id="@+id/buttonMoveB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="300dp"
        android:text="Siirrä" />

    <TextView
        android:id="@+id/textViewLTraining"
        android:layout_width="wrap_content"
